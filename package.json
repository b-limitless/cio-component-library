{
  "name": "@pasal/cio-component-library",
  "version": "32.0.0",
  "description": "",
  "scripts": {
    "rollup": "rollup -c --bundleConfigAsCjs",
    "pub": "npm publish --access public",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "clean": "del-cli ./dist/*",
    "publish:major": "npm version $(semver $npm_package_version -i major) && npm publish --access public --tag latest --no-email",
    "publish:minor": "npm version $(semver $npm_package_version -i minor) && npm publish --access public --tag latest --no-email",
    "publish:patch": "npm version $(semver $npm_package_version -i patch) && npm publish --access public --tag latest --no-email",
    "cr": "npm run clean && git add . && git commit -m \"$message\" && npm run rollup", 
    "final": "message=\"$message\" npm run cr && npm run publish:major"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@mui/icons-material": "^5.11.16",
    "@rollup/plugin-commonjs": "^25.0.1",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.1",
    "@storybook/addon-essentials": "^7.0.21",
    "@storybook/addon-interactions": "^7.0.21",
    "@storybook/addon-links": "^7.0.21",
    "@storybook/blocks": "^7.0.21",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^7.0.21",
    "@storybook/react-webpack5": "^7.0.21",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@types/react": "^18.2.12",
    "babel-loader": "^8.3.0",
    "css-loader": "^6.8.1",
    "del-cli": "^5.0.0",
    "prop-types": "^15.8.1",
    "rollup": "^3.25.1",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-sass": "^1.12.19",
    "rollup-plugin-scss": "^3.0.0",
    "sass": "^1.63.4",
